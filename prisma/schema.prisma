generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Measure {
  uuid            String    @id @default(uuid())
  image           String
  customer_code   String
  datetime        DateTime  @default(now())
  type            String
  value           Int
  confirmed       Boolean   @default(false)
}